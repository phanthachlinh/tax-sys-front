{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "esNext",
        "target": "es5",
        "jsx": "react",
        "types": ["react","node","jest","enzyme","redux-mock-store","react-router-dom","history","axios","@types/react-transition-group"],
        "lib": ["dom", "esnext"],
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports":true,
        "strictFunctionTypes":true,
        "strictNullChecks":true

    }
}
